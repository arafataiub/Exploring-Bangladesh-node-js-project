 <form method="post" href="/uedit">
      <div class="form-group">
      <label for="name">Name:</label>
      <input type="text" class="form-control" id="name" placeholder="Your full name" name="name" value="<%= result.user_name %>" required>
    </div>
    <div class="form-group">
      <label for="email">Email:</label>
      <input type="email" class="form-control" id="email" placeholder="Enter email" name="email" required value="<%= result.user_email %>">
    </div>
     <div class="form-group">
      <label for="phone">Phone No:</label>
      <input type="text" class="form-control" id="phone" placeholder="Enter Phone Number" name="phone" required value="<%= result.user_phoneno %>">
    </div>
