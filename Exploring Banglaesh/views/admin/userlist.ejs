      <tr>
        <th>User Name</th>
        <th>User Email</th>
        <th>User Phone</th>
        <th>Action</th>
      </tr>
    </thead>
    <tbody>
      <% for(var i = 0; i < result.length; i++){ %>
      <tr>
        <td><%= result[i].user_name %></td>
        <td><%= result[i].user_email %></td>
        <td><%= result[i].user_phoneno %></td>
